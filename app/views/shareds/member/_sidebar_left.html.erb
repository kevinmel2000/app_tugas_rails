<div class="panel panel-success">
	<div class="panel-heading">
		Account menu
	</div>
	<div class="panel-body">
		<ul class="list-unstyled">
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-home"></i></button>
				<%= link_to member_home_path do %>
				Dashboard
				<% end %>
			</li>
			<br>
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-car"></i></button>
				<%= link_to member_products_path(catalog_type: 'car') do %>
					Car
				<% end %>
			</li>
			<br>
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-car"></i></button>
				<%= link_to member_products_path(catalog_type: 'bike') do %>
					Bike
				<% end %>
			</li>
			<br>
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-car"></i></button>
				<%= link_to member_products_path(catalog_type: 'property') do %>
					Property
				<% end %>
			</li>
			<br>
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-car"></i></button>
				<%= link_to member_products_path(catalog_type: 'gadget') do %>
					Gadget
				<% end %>
			</li>
		</ul>
	</div>
</div>
<div class="panel panel-success">
	<div class="panel-heading">
		Contact User
	</div>
	<div class="panel-body">
		<ul class="list-unstyled">
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-map-marker"></i></button>
			</li>
			<br>
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-phone"></i></button>
			</li>
			<br>
			<li>
				<button class="btn btn-default btn-sm"><i class="fa fa-user"></i></button>
				<%#= @member.email %>
			</li>
		</ul>
	</div>
</div>