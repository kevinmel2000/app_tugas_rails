        <fieldset>
            <div class="form-group">
                <label class="col-md-4 control-label" for="example-chosen">Transmission</label>
                <div class="col-md-6">
                    <%= product_car.select :transmission, options_for_select(transmission, nil), {include_blank: 'Choose Transmission'}, class: 'form-control' %>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-4 control-label" for="example-chosen">Kilometer</label>
                <div class="col-md-6">
                    <%= product_car.text_field :kilometer, class: 'form-control', placeholder: 'Kilometer' %>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-4 control-label" for="example-chosen">Seat</label>
                <div class="col-md-6">
                    <%= product_car.select :seat, options_for_select(seat, nil), {include_blank: 'Choose Seat'}, class: 'form-control' %>                
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-4 control-label" for="example-chosen">Door</label>
                <div class="col-md-6">
                    <%= product_car.select :door, options_for_select(door, nil), {include_blank: 'Choose Door'}, class: 'form-control' %>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-4 control-label" for="example-chosen">Production Year</label>
                <div class="col-md-6">
                <%#= product_car.select :production_year, options_for_select(year, nil), {include_blank: 'Choose one'}, class: 'form-control' %>
                <%= product_car.select :production_year, (2000..Time.now.year).to_a, {include_blank: 'Choose Year'}, class: 'form-control' %>
             </div>
         </div>
         <div class="form-group">
            <label class="col-md-4 control-label" for="example-chosen">Number Plate</label>
            <div class="col-md-6">
                <%= product_car.text_field :number_plate, class: 'form-control', placeholder: 'Number Plate' %>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="example-chosen">Engine</label>
            <div class="col-md-6">
                <%= product_car.select :engine, options_for_select(engine, nil), {include_blank: 'Choose Engine'}, class: 'form-control' %>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="example-chosen">Fuel</label>
            <div class="col-md-6">
                <%= product_car.select :fuel, options_for_select(fuel, nil), {include_blank: 'Choose Fuel'}, class: 'form-control' %>
            </div>
        </div>
    </fieldset>