<section class="sub-top-header padding-top50">
  <div class="container">
    <div class="row">
      <div class="col-md-12"> 
        <h3>Masuk Anggota Sukabumi property</h3>
      </div>
    </div>
  </div>
</section>

<section class="page-sign-in-up">
  <div class="container">
    <div class="row">
      <div class="col-md-4"></div>
      <div class="col-md-4">
        <div class="panel panel-primary">
          <div class="panel-heading">
            EDIT <%= resource_name.to_s.humanize %>
          </div>
          <%= devise_error_messages! %>
          <div class="panel-body">
            <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
            <div class="form-group form-login">
              <label class="label-form-login">Email</label>
              <%= f.email_field :email, class: 'form-control', autofocus: true %>
              <div class="br10"></div>
              <label class="label-form-login">Username</label>
              <%= f.text_field :username, class: 'form-control', autofocus: true %>
              <div class="br10"></div>
              <label class="label-form-login">Password</label>
              <%= f.password_field :password, class: 'form-control', autocomplete: "off" %>
              <div class="br10"></div>
              <label class="label-form-login">New Password</label>
              <%= f.password_field :current_password, class: 'form-control', autocomplete: "off" %>
              <div class="br10"></div>
              <div class="input-group">
                <label></label>
                <%= f.submit "Update", class: 'btn btn-success' %>
              </div>
            </div>
            <% end %>
          </div>
          <%= link_to "Back", :back %>
        </div>
      </div>
      <div class="col-md-4"></div>
    </div>
  </div>
</section>